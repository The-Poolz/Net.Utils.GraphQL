<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <PackageId>Net.Utils.GraphQL</PackageId>
    <Title>Utilities for working with GraphQL responses.</Title>
    <Version>1.0.0</Version>
    <Authors>ArdenHide, Lomet</Authors>
    <Company>The-Poolz</Company>
    <Product>Net.Utils.GraphQL</Product>
    <Description>Utilities for working with GraphQL responses.</Description>
    <RepositoryUrl>https://github.com/The-Poolz/Net.Utils.GraphQL</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>graphql;utilities</PackageTags>
    <IncludeSymbols>True</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GraphQL.Client.Abstractions" Version="6.1.0" />
  </ItemGroup>

</Project>
